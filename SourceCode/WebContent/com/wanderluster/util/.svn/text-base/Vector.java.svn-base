package com.wanderluster.util;

public class Vector {

	private int[] indices;
	private double[] values;

	public Vector(int[] indices, double[] values) {
		this.indices = indices;
		this.values = values;
	}

	public int[] getIndices() {
		return indices;
	}

	public double[] getValues() {
		return values;
	}

}
